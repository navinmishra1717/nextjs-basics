{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["next/core-web-vitals", "plugin:@typescript-eslint/recommended"],
  "rules": {
    "import/prefer-default-export": "off",
    "no-restricted-syntax": "off",
    "class-methods-use-this": "off",
    "max-len": [
      "error",
      {
        "code": 250
      }
    ]
  }
}
